#!/bin/bash -v
cat $1 | sed -e "s/@[ \t]*([ \t]*\*[ \t]*)/@*/g"|sed -e "s/\^~/~\^/g" >  $1.add
tokenizer $1.add | sed -e '/^[ \t]*$/d' | sed -e "s/'d[ \t]*\([0-9]\)/'d\1/g" | sed -e "s/'d0\([0-9]\)/'d\1/g" | sed -e "s/^0\+\(.\)/\1/g" |grep -v "\`line" | sed -e "s/\('[dDbBoOhH][0-9]\+\)_/\1/g" |sed -e "s/'D/'d/g"| sed -e "s/'O/'o/g" | sed -e "s/'H/'h/g" | sed -e "s/'B/'b/g" > $1.tok
